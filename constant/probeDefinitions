FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      loggingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceElevationAnyName
{
    type waveGauge;

    pointDistribution lineDistribution;
    N 179;
    linestart (0.10 -0.3 0.05);
    lineend   (17.9 -0.3 0.05);
    add       ( 0 0.45 0);
    axis      y;
    stretch 1.;
}


