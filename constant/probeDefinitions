FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      loggingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceElevationAnyName
{
    type waveGauge;

    pointDistribution lineDistribution;
    N 20;
    linestart   (0.10 1e-12 -0.3);
    lineend     (35.9 1e-12 -0.3);
    add         (0 0 0.45);
    axis        z;
    stretch     1.0;
}


